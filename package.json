{
  "name": "@server-template/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "init": "node tools/init.js",
    "reset": "nx reset",
    "graph": "nx graph",
    "format": "nx format:write --all",
    "affected:apps": "npx nx show projects --affected --type=app",
    "docker-base:local": "docker build -f Dockerfile.base -t server-template/base:local .",
    "update-generator": "ts-node tools/update-generator-schema.ts",
    "test": "npx nx run-many --all --target=test",
    "docker:dev": "docker compose -p server-template -f docker-compose.yml up",
    "docker:up": "docker compose -p server-template -f docker-compose.yml up -d --build",
    "docker:down": "docker compose -p server-template -f docker-compose.yml down",
    "docker:infra:up": "docker compose -p server-template -f docker-compose.infra.yml up -d --build",
    "docker:infra:down": "docker compose -p server-template -f docker-compose.infra.yml down",
    "prisma:deploy": "cross-env DOTENV_CONFIG_PATH=./libs/prisma/src/schema/data-management/.env npx prisma migrate deploy --schema=libs/prisma/src/schema/data-management/schema.prisma",
    "prisma:dev": "cross-env DOTENV_CONFIG_PATH=./libs/prisma/src/schema/data-management/.env npx prisma migrate dev --name data-management  --schema=libs/prisma/src/schema/data-management/schema.prisma",
    "prisma:generate": "cross-env DOTENV_CONFIG_PATH=./libs/prisma/src/schema/data-management/.env npx prisma generate --schema=libs/prisma/src/schema/data-management/schema.prisma",
    "prisma:dev:create-only": "cross-env DOTENV_CONFIG_PATH=./libs/prisma/src/schema/data-management/.env npx prisma migrate dev --create-only --name data-management --schema=libs/prisma/src/schema/data-management/schema.prisma"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^11.1.9",
    "@nestjs/core": "^11.1.9",
    "@nestjs/microservices": "^11.1.9",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/typeorm": "^11.0.0",
    "@nx/devkit": "22.0.4",
    "@nx/plugin": "^22.0.4",
    "@prisma/client": "^6.19.0",
    "axios": "^1.13.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "config": "^4.1.1",
    "glob": "^11.1.0",
    "ioredis": "^5.8.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sqlite3": "^5.1.7",
    "tar-fs": "^3.1.1",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@nx/eslint": "22.0.4",
    "@nx/eslint-plugin": "22.0.4",
    "@nx/jest": "22.0.4",
    "@nx/js": "22.0.4",
    "@nx/nest": "22.0.4",
    "@nx/node": "22.0.4",
    "@nx/web": "22.0.4",
    "@nx/webpack": "22.0.4",
    "@nx/workspace": "22.0.4",
    "@swc-node/register": "~1.11.1",
    "@swc/cli": "~0.7.9",
    "@swc/core": "~1.15.2",
    "@swc/helpers": "~0.5.17",
    "@swc/jest": "~0.2.39",
    "@types/config": "^3.3.5",
    "@types/jest": "^30.0.0",
    "@types/node": "~24.10.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "javascript-obfuscator": "^4.1.1",
    "jest": "^30.2.0",
    "jest-environment-node": "^30.2.0",
    "nx": "22.0.4",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "ts-node": "10.9.2",
    "tslib": "^2.8.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.47.0",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  },
  "workspaces": [
    "apps/*",
    "libs/*",
    "plugins"
  ]
}
